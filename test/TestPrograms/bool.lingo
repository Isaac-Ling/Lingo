-- Boolean
Bool := T + T

#check Bool

true : Bool
true := inl(*)

false : Bool
false := inr(*)

#check true
#check false

-- Conditional
ifThenElse : {C : U} -> Bool -> C -> C -> C
ifThenElse b c c' := ind[Bool](C, a. c, a. c', b)

#check ifThenElse

not : Bool -> Bool
not b := ifThenElse b false true

#check not false
