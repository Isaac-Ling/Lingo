-- Type theoretic axiom of choice
ac : (A : U) -> (B : U) -> (R : A -> B -> U) -> ((a : A) -> (b : B) x (R a b)) -> ((f : A -> B) x ((a : A) -> (R a (f a))))
ac := \A. \B. \R. \g. (\(a : A). fst B (\(b : B). R a b) (g a), \(a : A). snd B (\(b : B). R a b) (g a))

-- Transport
transport : (A : U) -> (P : (y : A) -> U) -> (a : A) -> (b : A) -> a = b -> P a -> P b
transport := \A. \P. \a. \b. \p. ind[=[A]](a. b. p. P a -> P b, z. id (P z), a, b, p)

#check transport
