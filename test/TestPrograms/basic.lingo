-- Identity
id : {A : U} -> A -> A
id := \(a : A). a

#check id 32

-- Projections
fst : {A : U} -> {B : U} -> A x B -> A
fst := \p. ind[A x B](A, a. b. a, p)

#check fst

dsnd : {A : U} -> (B : A -> U) -> (p : (a : A) x B a) -> B (fst p)
dsnd := \B. \p. ind[(a : A) x B a](p. B (fst p), a. b. b, p)

snd : {A : U} -> {B : U} -> A x B -> B
snd := \{A}. \{B}. \p. dsnd (\(a : A). B) p

#check fst (*, _|_)
#check snd (*, _|_)
