-- Identity
id : {A : U} -> A -> A
id := \a. a

#check id 32

-- Projections
fst : {A : U} -> (B : A -> U) -> (a : A) x B a -> A
fst := \B. \p. ind[(a : A) x B a](A, a. b. a, p)

#check fst

snd : {A : U} -> (B : A -> U) -> (p : (a : A) x B a) -> B (fst B p)
snd := \B. \p. ind[(a : A) x B a](p. B (fst B p), a. b. b, p)

#check snd
