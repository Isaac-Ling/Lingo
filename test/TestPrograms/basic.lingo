-- Identity
id : {A : U} -> A -> A
id := \(a : A). a

#check id 32

-- Projections
dfst : (A : U) -> (B : A -> U) -> (a : A) x B a -> A
dfst := \A. \B. \p. ind[(a : A) x B a](A, a. b. a, p)

#check dfst

dsnd : (A : U) -> (B : A -> U) -> (p : (a : A) x B a) -> B (dfst A B p)
dsnd := \A. \B. \p. ind[(a : A) x B a](p. B (dfst A B p), a. b. b, p)

#check dsnd

fst : {A : U} -> {B : U} -> A x B -> A
fst := \{A}. \{B}. \p. dfst A (\(a : A). B) p

#check fst (*, _|_)

snd : {A : U} -> {B : U} -> A x B -> B
snd := \{A}. \{B}. \p. dsnd A (\(a : A). B) p

#check snd (*, _|_)

-- Composition
comp : {A : U} -> {B : U} -> {C : U} -> (B -> C) -> (A -> B) -> A -> C
comp := \g. \f. \a. g (f a)

#check comp
