-- Projections

fst : (A : U) -> (B : U) -> A x B -> A
fst := \A. \B. \p. ind[A x B](A, a. b. a, p)

snd : (A : U) -> (B : U) -> A x B -> B
snd := \A. \B. \p. ind[A x B](B, a. b. b, p)

-- Type theoretic axiom of choice
--ac : (A : U) -> (B : U) -> (R : A -> B -> U) -> ((a : A) -> (b : B) x (R a b)) -> ((f : A -> B) x ((a : A) -> (R a (f a))))
ac : (A : U) -> (B : U) -> (R : A -> B -> U) -> ((a : A) -> (b : B) x (R a b)) -> U
ac := \A. \B. \R. \g. 1

#check fst
#check ac
