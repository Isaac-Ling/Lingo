-- Projections
fst := \(A : U). \(B : U). \(p : A x B). ind[A x B](A, a. b. a, p)
snd := \(A : U). \(B : U). \(p : A x B). ind[A x B](B, a. b. b, p)

-- Type theoretic axiom of choice
ac : (A : U) -> (B : U) -> (R : A -> B -> U) -> ((a : A) -> (b : B) x (R a b)) -> ((f : A -> B) x ((a : A) -> (R a (f a))))
--ac := \(A : U). \(B : U). \(R : A -> B -> U). \(g : (a : A) -> (b : B) x (R a b)). 1

#check fst
#check ac
